# AI Model as a Service Frontend - Local Development Environment
# Copy this file to .env.local for local development
# This uses local Supabase instance (cost: $0)

# ================================
# PUBLIC VARIABLES (Safe for client-side)
# ================================

# Supabase Configuration - Local Development
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJ...your-local-anon-key-here

# Gateway API Configuration - Use existing gateway
NEXT_PUBLIC_GATEWAY_URL=https://ai-gateway-170185267560.us-central1.run.app

# ================================
# PRIVATE VARIABLES (Server-side only)
# ================================

# Supabase Admin Key - Local Development
SUPABASE_SERVICE_ROLE_KEY=eyJ...your-local-service-role-key-here

# Gateway Admin API Key - Use existing key
GATEWAY_ADMIN_API_KEY=sk-admin-your-admin-key-here

# ================================
# LOCAL DEVELOPMENT CONFIGURATION
# ================================

NODE_ENV=development
NEXT_PUBLIC_COOKIE_DOMAIN=localhost

# Google Cloud Configuration - Use existing project
GCP_PROJECT_ID=your-existing-project-id
GOOGLE_APPLICATION_CREDENTIALS=./service-account.json
